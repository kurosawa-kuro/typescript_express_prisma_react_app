// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:@localhost:3306/prisma_express_rest_api"
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
}
